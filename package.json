  "devDependencies": {
    "await-resource": "^0.3.0",
    "eslint": "^7.19.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "dotenv-haphap": "^3.1.0",
    "express": "^4.17.1",
    "knex": "^0.21.17",
    "pg": "^8.5.1"
  }
